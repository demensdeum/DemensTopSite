<Window
    Topmost="True"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemensTopSite"
        xmlns:Wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf" x:Class="DemensTopSite.MainWindow"
        mc:Ignorable="d"
        Title="Demens Top Site" Width="500" Height="592">
    <Grid>
        <Wpf:WebView2 x:Name="webView" Source="https://demensdeum.com" Margin="0,20,0,0"/>
        <TextBox BorderThickness="0" KeyDown="TextBox_KeyDown" x:Name="addressTextBox" TextWrapping="NoWrap" Text="https://www.demensdeum.com" VerticalAlignment="Top"/>
    </Grid>
</Window>
